types{
    Actor-Locatable,
    Location-Object,
    Locatable-Object
}

objects{
    robot-Actor,
    roomba-Actor,
    livingR-Location,
    kitchen-Location,
    bedR-Location
}

agents{
    robot,
    roomba{CleanModel},
}

passive{}

fluents{
    at(Locatable,Location),
    blocked(),
    liquid(Location),
    dirt(Location),
    roomba_wet(),
    blocked_kitchen(),
    blocked_bedroom()
}

constants{
    connected(livingR, kitchen)
}


