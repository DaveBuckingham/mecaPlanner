types{
}

objects{
    a - Object,
}

agents{ a }


fluents{
    o(), l()
}


initially{
    *w1 <- {o(),l()},
    w2 <- {o()},
    w3 <- {l()},
    w4 <- {},
    B[a] <- {(w1,w1),(w1,w2),(w1,w3),(w1,w4),
             (w2,w1),(w2,w2),(w2,w3),(w2,w4),
             (w3,w1),(w3,w2),(w3,w3),(w3,w4),
             (w4,w1),(w4,w2),(w4,w3),(w4,w4)
            },
    K[a] <- {(w1,w1),(w1,w2),(w1,w3),(w1,w4),
             (w2,w1),(w2,w2),(w2,w3),(w2,w4),
             (w3,w1),(w3,w2),(w3,w3),(w3,w4),
             (w4,w1),(w4,w2),(w4,w3),(w4,w4)
    }
}

goals{
    false
}

actions{
    open(){
        owner{a},
        observes(?x-Object){a},
        causes{o() if ~l()},
        determines{o()}
    }

}
